   <script>
     function myMap() {
    var mapOptions = {
      center: new google.maps.LatLng(45.9192139, 6.141949899999986),
      zoom: 10,
      mapTypeId: google.maps.MapTypeId.HYBRID
    }
    var myCenter = new google.maps.LatLng(45.91862260000001,6.157968500000038);
    var map = new google.maps.Map(document.getElementById("map"), mapOptions);
    var marker = new google.maps.Marker({position:myCenter});
    marker.setMap(map);
    var myCenter2 = new google.maps.LatLng(45.9144629,6.114150099999961);
    var marker = new google.maps.Marker({position:myCenter2});
        marker.setMap(map);
  }
  </script>
    
